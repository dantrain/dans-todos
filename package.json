{
  "name": "dans-todos",
  "version": "0.1.0",
  "author": "Dan Train",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start:tsc": "tsc -b -w --preserveWatchOutput",
    "start": "yarn run generate && concurrently -k \"yarn run start:db\" \"wait-on tcp:5432 && yarn run start:redis\" \"wait-on tcp:6379 && yarn run start:tsc\" \"razzle start\"",
    "build": "yarn run generate && tsc -b && razzle build",
    "test": "razzle test --env=jsdom",
    "clean": "rimraf build",
    "type-check": "tsc --noEmit",
    "prettify": "prettier --write 'src/**/*.ts' 'src/**/*.tsx'",
    "start:prod": "NODE_ENV=production node build/server.js",
    "start:db": "docker-compose run --publish 5432:5432 db",
    "start:redis": "docker-compose run --publish 6379:6379 redis",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/server/schema",
    "generate:relay": "relay-compiler",
    "generate": "yarn run generate:prisma && yarn run generate:nexus && yarn run generate:relay",
    "postinstall": "yarn run generate:prisma",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down --rmi all --volumes",
    "heroku:db:schema": "heroku pg:psql --app dans-todos -f schema.sql",
    "report": "source-map-explorer build/public/static/js/*.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@nexus/schema": "^0.16.0",
    "@prisma/client": "2.9.0",
    "apollo-server-express": "^2.18.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.1",
    "google-auth-library": "^6.1.1",
    "graphql": "^15.3.0",
    "graphql-relay": "^0.6.0",
    "helmet": "^4.1.1",
    "history": "^5.0.0",
    "lodash": "^4.17.20",
    "material-ui-popup-state": "^1.7.0",
    "nexus-plugin-prisma": "^0.22.0",
    "notistack": "^1.0.1",
    "pubsub-js": "^1.9.0",
    "razzle-heroku": "^3.0.0",
    "razzle-plugin-workbox": "^1.0.3",
    "react": "0.0.0-experimental-94c0244ba",
    "react-dom": "0.0.0-experimental-94c0244ba",
    "react-error-boundary": "^3.0.2",
    "react-helmet": "^6.1.0",
    "react-relay": "0.0.0-experimental-183bdd28",
    "react-router-dom": "0.0.0-experimental-ffd8c7d0",
    "react-transition-group": "^4.4.1",
    "redis": "^3.0.2",
    "relay-runtime": "^10.0.1",
    "serialize-javascript": "^5.0.1",
    "useragent": "^2.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@prisma/cli": "2.9.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.8",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/connect-redis": "^0.0.14",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/gapi.auth2": "^0.0.52",
    "@types/graphql-relay": "^0.6.0",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.164",
    "@types/node": "^14.11.10",
    "@types/pubsub-js": "^1.8.1",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^6.1.0",
    "@types/react-relay": "^7.0.13",
    "@types/react-router-dom": "^5.1.6",
    "@types/redis": "^2.8.28",
    "@types/relay-runtime": "^10.0.4",
    "@types/serialize-javascript": "^4.0.0",
    "@types/useragent": "^2.1.1",
    "@types/webpack-env": "^1.15.3",
    "apollo-server-plugin-base": "^0.10.2",
    "babel-plugin-relay": "^10.0.1",
    "concurrently": "^5.3.0",
    "prettier": "^2.1.2",
    "razzle": "^3.3.7",
    "relay-compiler": "^10.0.1",
    "relay-compiler-language-typescript": "^13.0.1",
    "relay-config": "^10.0.1",
    "rimraf": "^3.0.2",
    "source-map-explorer": "^2.5.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "typescript-plugin-css-modules": "^2.7.0",
    "wait-on": "^5.2.0"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "engines": {
    "node": "12.x"
  }
}
