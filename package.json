    "express-session": "^1.17.1",
    "google-auth-library": "^6.1.6",
    "graphql": "^15.5.0",
    "graphql-relay": "^0.6.0",
    "helmet": "^4.4.1",
    "history": "^5.0.0",
    "lodash": "^4.17.20",
    "material-ui-popup-state": "^1.7.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.29.0",
    "notistack": "^1.0.3",
    "pubsub-js": "^1.9.2",
    "razzle-heroku": "^3.0.0",
    "razzle-plugin-workbox": "^1.0.3",
    "react": "^0.0.0-experimental-27659559e",
    "react-dom": "^0.0.0-experimental-27659559e",
    "react-error-boundary": "^3.1.0",
    "react-helmet": "^6.1.0",
    "react-relay": "^0.0.0-experimental-4c4107dd",
    "react-router-dom": "^0.0.0-experimental-ffd8c7d0",
    "react-transition-group": "^4.4.1",
