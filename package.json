{
  "name": "todo-next",
  "version": "0.1.0",
  "author": "Dan Train",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start:tsc": "tsc -b -w --preserveWatchOutput",
    "start": "yarn start:tsc & razzle start",
    "build": "tsc -b && razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/server/schema",
    "generate": "yarn run generate:prisma && yarn run generate:nexus"
  },
  "dependencies": {
    "@nexus/schema": "^0.15.0",
    "apollo-server-express": "^2.17.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "graphql": "^15.3.0",
    "helmet": "^4.1.1",
    "nexus-plugin-prisma": "^0.18.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.4",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.4",
    "@types/webpack-env": "^1.15.2",
    "razzle": "^3.3.0-canary.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "typescript-plugin-css-modules": "^2.5.0"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
